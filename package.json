{
  "name": "snailbird-content",
  "version": "0.0.1",
  "description": "Content server for media for snailbird",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "run": "node ./bin/src",
    "debug": "node --inspect ./bin/src",
    "br": "npm run build && npm run run",
    "bd": "npm run build && npm run debug",
    "test": "npm run build && node --test ./bin/test/index.test.js",
    "testi": "npm run build && node --test --inspect ./bin/test/index.test.js"
  },
  "files": [
    "./bin/**/*"
  ],
  "author": "Daniel Harvey",
  "type": "module",
  "license": "Apache-2.0",
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/node": "^22.9.0",
    "typescript": "^5.6.3"
  },
  "dependencies": {
    "express": "^4.21.1"
  }
}
